<h1>Modules</h1>
<table>
    <tr>
    <th>Module ID</th>
    <th>Name</th>
    <th>Credits</th>
    <th>Update</th>
    <th>Students</th>
    </tr>
    <% module.forEach((module) =>
    { %>
    <tr>
    <td><%= module.mid %></td>
    <td><%= module.name %></td>
    <td><%= module.credits %></td>
    <td><a href="">Update</a></td>
    <td><a href="/listStudents">List</a></td>
    </tr>
    <% }) %>
</table>

<a href="http://localhost:3000/">Home</a>